{
  "name": "dump_describes",
  "version": "0.3.3",
  "description": "View all `describe` and `it` blocks in a test suite",
  "bin": {
    "dump_describes": "./bin/dump_describes"
  },
  "main": "./src/index.js",
  "scripts": {
    "clean": "rm -rf build coverage",
    "compile": "babel -d build src",
    "coverage": "JASMINE_CONFIG_PATH=jasmine.json istanbul cover jasmine --captureExceptions",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "lint": "eslint bin spec src",
    "specs": "JASMINE_CONFIG_PATH=jasmine.json jasmine && npm run suites",
    "suites": "bin/dump_describes -t spec/dump_describes_spec.js -d spec -v --html && bin/dump_describes -t spec/dump_describes_spec.js -d spec -v --md",
    "test": "npm run clean && npm run lint && npm run compile && npm run specs",
    "watch": "bash ./scripts/watch.bash"
  },
  "keywords": [
    "describes",
    "it",
    "jasmine",
    "mocha",
    "jest",
    "esprima",
    "unit testing",
    "testing",
    "test",
    "suite"
  ],
  "author": "Benjamin Toll <benjam72@yahoo.com> (http://www.benjamintoll.com)",
  "license": "GPLv3",
  "dependencies": {
    "esprima": ">=4.x",
    "jasmine": "^3.4.0",
    "onf-logger": "^0.1.0",
    "onf-static": "git+https://github.com/btoll/onf-static.git",
    "yargs": "^3.29.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btoll/dump_describes.git"
  },
  "bugs": {
    "url": "https://github.com/btoll/dump_describes/issues"
  },
  "homepage": "https://github.com/btoll/dump_describes#readme",
  "devDependencies": {
    "babel-core": "^6.6.4",
    "babel-preset-es2015": "^6.6.0",
    "coveralls": "^2.11.8",
    "eslint": "^2.2.0",
    "istanbul": "^0.4.2",
    "jasmine-core": "^2.4.1"
  }
}
